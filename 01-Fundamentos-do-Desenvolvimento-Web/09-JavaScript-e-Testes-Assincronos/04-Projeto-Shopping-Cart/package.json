    "url": "https://github.com/vitorbss12/Trybe-Exercicios-e-Projetos/tree/master/01-Fundamentos-do-Desenvolvimento-Web"
  },
  "homepage": "https://github.com/vitorbss12/Trybe-Exercicios-e-Projetos/tree/master/01-Fundamentos-do-Desenvolvimento-Web#readme",
  "dependencies": {
    "eslint-config-trybe-fundamentals": "1.1.0",
    "jest": "24.9.0",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0"
  }
}
